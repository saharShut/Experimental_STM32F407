# Tool invocations
LVGS_Test.elf: $(OBJS) $(USER_OBJS) makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU Arm Cross C++ Linker'
	arm-none-eabi-g++ -mcpu=cortex-m4 -mthumb -mfloat-abi=hard -mfpu=fpv4-sp-d16 -Og -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-move-loop-invariants -Wall -Wextra -g -T "C:\Users\Alex\eclipse-workspace\LVGS_Test\STM32F407VETx_FLASH.ld" -nostartfiles -Wl,-Map,"LVGS_Test.map" --specs=nano.specs --specs=nosys.specs -o "LVGS_Test.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

LVGS_Test.bin: LVGS_Test.elf makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Invoking: GNU Arm Cross Create Flash Image'
	arm-none-eabi-objcopy -O binary "LVGS_Test.elf"  "LVGS_Test.bin"
	@echo 'Finished building: $@'
	@echo ' '

LVGS_Test.siz: LVGS_Test.elf makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Invoking: GNU Arm Cross Print Size'
	arm-none-eabi-size --format=berkeley --totals "LVGS_Test.elf"
	@echo 'Finished building: $@'
	@echo ' '

